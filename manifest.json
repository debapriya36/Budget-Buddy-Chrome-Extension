{
  "manifest_version": 2,
  "name": "Budget Buddy",
  "description": "Budget Buddy-This extension tracks your spendings.",
  "version": "0.0.1",
  "icons": {
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "popup.html"
  },

  "permissions": [
          "storage",
          "contextMenus"
        ],

  "options_page": "options.html"
}
